{% include 'incl/header.twig' %}

<body>
    {% include 'incl/nav.twig' %}

    <img class="flash-img" src="https://www.publicdomainpictures.net/pictures/270000/velka/ocean-wave-web-banner.jpg?w=550&h=325&crop-to-fit">


    <div class="main" role="main">
        <div class="single-technology">
            <div class="tech-menu">
                <h2>Meny</h2>
                    {% for page in pages("technology")|sort_by("time")|reverse if not page.hidden %}
                    <div class="post">
                        <h3><a href="{{ page.url }}">{{ page.title }}</a></h3>
                    </div>
                    {% endfor %}
            </div>
            <div class ="tech-content">
                {{ content }}  
            </div>
        </div>
    </div>

    
    {% include 'incl/footer.twig' %}
</body>

</html>
